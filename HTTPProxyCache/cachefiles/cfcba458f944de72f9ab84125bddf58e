    jQuery("#sportBoxFront ul").addClass('ui-tabs-nav');
    jQuery("#sportBoxFront > div").addClass('ui-tabs-panel');
    jQuery("#sportBoxFront ul.ui-tabs-nav #SPboxTab_today").addClass('ui-tabs-selected');
    jQuery('#sportBoxFront select option:selected').removeAttr('selected');
    jQuery('#sportBoxFront select #SPbox_yesterday_3').attr('selected','selected');
    jQuery('#sportBoxFront select #SPbox_today_3').attr('selected','selected');
    jQuery('#sportBoxFront select #SPbox_tomorrow_3').attr('selected','selected');
    jQuery("#sportBoxFront .sportTable_tab_yesterday").addClass('ui-tabs-hide');
    jQuery("#sportBoxFront .sportTable_tab_tomorrow").addClass('ui-tabs-hide');
    jQuery("#sportBoxFront .sportTable_tab_today div:nth-child(4)").removeClass('ui-tabs-hide');
    
    jQuery("#sportBoxFront ul.ui-tabs-nav li a").click(function() {
        jQuery("#sportBoxFront .ui-tabs-panel").addClass('ui-tabs-hide'); 
        jQuery("#sportBoxFront .sportTable_tab_"+jQuery(this).attr('name')).removeClass('ui-tabs-hide');
        var optionName = jQuery("#sportBoxFront .sportTable_tab_"+jQuery(this).attr('name')+" select").find(":selected").attr('id');
        jQuery("#"+optionName+"_html").removeClass('ui-tabs-hide');
        jQuery("#sportBoxFront ul.ui-tabs-nav li").removeClass('ui-tabs-selected');
        jQuery(this).parent().addClass('ui-tabs-selected');
    });
    
    jQuery(".ui-tabs-panel select").bind('change', function() {
        jQuery(this).siblings().addClass('ui-tabs-hide');
          var optionName = jQuery(this).find(":selected").attr('id');
        var optionID = optionName.slice(optionName.length-1, optionName.length);
        jQuery("#sportBoxFront select").selectedIndex = optionID;
        jQuery('#sportBoxFront select option:selected').removeAttr('selected');
        jQuery('#sportBoxFront select #SPbox_yesterday_'+optionID).attr('selected','selected');
        jQuery('#sportBoxFront select #SPbox_today_'+optionID).attr('selected','selected');
        jQuery('#sportBoxFront select #SPbox_tomorrow_'+optionID).attr('selected','selected');
        jQuery("#"+optionName+"_html").removeClass('ui-tabs-hide');
    });
});
</script></div></td></tr>
