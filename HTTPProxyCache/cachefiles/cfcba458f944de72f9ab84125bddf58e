		<script>
            var track_wemf = function(url, callback)
            {
                url = (url==null) ? '' : '/'+url;
                
				var WEMF="http://derbund.wemfbox.ch/cgi-bin/ivw/CP/";
				track_wemf_do(WEMF+''+url);

			 
				var WEMF2="http://newsnetz.wemfbox.ch/cgi-bin/ivw/CP/derbund/";
				track_wemf_do(WEMF2+''+url);
            
            }

            var track_wemf_do = function(url2, callback)
            {
                var img = new Image();
                if(typeof callback === "function") {
                    $(img).load(callback);
                }
                img.src = url2 + "?rnd=" + parseInt(new Date().getTime() * Math.random(), 10);
            }
		</script>
		
