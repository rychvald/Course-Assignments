    <script type="text/javascript">
    	function changeChart(iChartId) {
    
    		oImgs = document.getElementById('chartimgs');
    		oLinks = document.getElementById('chartlinks');
    		aImg = oImgs.getElementsByTagName('img');
    		aTds = oLinks.getElementsByTagName('td');
    
    		for (x=0;x<aImg.length;x++) {
    			aImg[x].style.display='none';
    			aTds[x].className='passiv';
    		}
    
    		document.getElementById(iChartId).style.display='inline';
    		document.getElementById('t'+iChartId).className='aktiv';
    
    	}
    </script>

